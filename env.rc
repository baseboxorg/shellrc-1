export EDITOR=vim
if [ $COLUMNS -gt 80 ]; then
  MANWIDTH=80
else
  MANWIDTH=$COLUMNS
fi
export MANWIDTH

if [ -x ~/go/bin/go ]; then
  export GOPATH=~/gocode
fi
FIGNORE='.o:.lo:.out:.slo:.gcda:.gcno'

export PASSWORD_STORE_X_SELECTION=primary

export ROWS COLUMNS LINES
PAGER=less
export PAGER

export INPUTRC=$HOME/.inputrc
